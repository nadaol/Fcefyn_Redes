; -----------------------------------------------------------------------------

@ IN SOA root admin.root (
            2020051202      ; Serial
            1h              ; Refresh
            15m             ; Retry
            1d              ; Expire
            1h );           ; Negative Cache TTL

@   IN    NS        com
@   IN    NS        edu
; -----------------------------------------------------------------------------
