$TTL 1h
; -----------------------------------------------------------------------------
; SOA for .com
; -----------------------------------------------------------------------------

@ IN SOA ns1.com admin.com. (
            2020053002      ; Serial
            1h              ; Refresh
            15m             ; Retry
            1d              ; Expire
            1h );           ; Negative Cache TTL

; -----------------------------------------------------------------------------
; zone records
; -----------------------------------------------------------------------------
@   IN    NS        ns1.com.

; -----------------------------------------------------------------------------
; pointer records
; -----------------------------------------------------------------------------

ns1.com.                 1800    IN    A         30.0.0.3
ns1.com.                 1800    IN    AAAA      30::3
pc1.com.                1800    IN    A         30.1.0.3
pc1.com.                  1800    IN    AAAA      30:1::3
pc2.com.                 1800    IN    A         30.1.0.4
pc2.com.                 1800    IN    AAAA      30:1::4
strapi.server.com.                 1800    IN    A         30.2.0.3
strapi.server.com.                 1800    IN    AAAA      30:2::3

